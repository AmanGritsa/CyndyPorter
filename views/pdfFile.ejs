<html>

<head>
    <title>Image Details</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css">
    <style>
        @media print {
            body {
                font-family: 'Lato', sans-serif;
            }

            .cyndu-image-section {
                margin: 20px 0px;
            }

            .cyndu-image-section img {
                width: 300px;
                height: 300px;
            }

            .color-section {
                width: 20px;
                height: 20px;
                background: #f9ca53;
                display: block;
                text-align: center;
                color: #fff;
                font-size: 11px;
                line-height: 20px;
            }

            .color-name h4 {
                margin: 0;
                font-size: 16px;
                font-weight: 600;
                text-transform: capitalize;
            }


            .marginbottom {
                margin-bottom: 10px;
            }

            .color-anlysis {
                margin: 20px 0px;
            }

            .cyndy-portal-pdf {
                margin: 20px 0px;
            }
        }



        body {
            font-family: 'Lato', sans-serif;
        }

        .cyndu-image-section {
            margin: 20px 0px;
        }

        .cyndu-image-section img {
            width: 300px;
            height: 300px;
        }

        .color-section {
            width: 20px;
            height: 20px;
            background: #fff !important;
            display: block;
            text-align: center;
            color: #fff;
            font-size: 11px;
            line-height: 20px;
        }

        .color-name h4 {
            margin: 0;
            font-size: 16px;
            font-weight: 600;
            text-transform: capitalize;
        }


        .marginbottom {
            margin-bottom: 10px;
        }

        .color-anlysis {
            margin: 20px 0px;
        }

        .cyndy-portal-pdf {
            margin: 20px 0px;
        }

        .cyndu-details-section {
            margin: 20px 0px;
        }


        .bold-hading-sec {
            font-weight: bold;
        }
    </style>

</head>

<body>

    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
        <div class="container">
            <div class="cyndy-portal-pdf">
                <div class="cundy-logo">
                    <img src="http://ec2-18-218-245-183.us-east-2.compute.amazonaws.com/assets/img/logo-dark.png" class="img-responsive center-block">
                </div>
                <div class="">


                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        <div class="cyndu-details-section">
                            <div class="details-section">
                                <p class="bold-hading-sec">Email Id</p>
                                <p class="ans-sec">
                                    <%= user.userDetails.email %>
                                </p>
                            </div>
                            <div class="details-section">
                                <p class="bold-hading-sec">User Name</p>
                                <p class="ans-sec">
                                    <%= user.userDetails.userName %>
                                </p>
                            </div>
                            <div class="details-section">
                                <p class="bold-hading-sec">Phone Number</p>
                                <p class="ans-sec">
                                    <%= user.userDetails.phoneNumber %>
                                </p>
                            </div>
                            <div class="details-section">
                                <p class="bold-hading-sec">Stylist Name</p>
                                <p class="ans-sec">
                                    <%= user.userDetails.stylistName %>
                                </p>
                            </div>
                        </div>
                    </div>

                    <div class="col-xs-6 col-sm-6 col-md-6 col-lg-6">
                        <div class="cyndu-image-section">
                            <img src="<%= user.image.imageUrl %>" class="img-responisve">
                        </div>
                    </div>

                </div>
                <div class="color-anlysis">
                    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>hair Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                    <div class="color-section">
                                        <span class="color-section"></span>
                                    </div>
                                </div>
                                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                    <div class="color-section">
                                        <span class="color-section"></span>
                                    </div>
                                </div>
                                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                    <div class="color-section">
                                        <span class="color-section"></span>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>Skin Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                    <% for(var i=0; i < user.colors.skin.length; i++) { %>
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.skin[i] %> !important'></span>
                                        </div>
                                        <% } %>
                                </div>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>eyes Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.eyes.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">

                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.eyes[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <!-- </div> -->
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>lips Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.lips.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">

                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.lips[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>metals Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.metals.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.metals[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>reds Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.reds.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.reds[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>neutrals Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.neutral.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.neutral[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>wow Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.wow.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.wow[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>accent Color</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.accent.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.accent[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom" style="padding-top: 30px">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>All Colors</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.avoids.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.avoids[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                        <div class="row marginbottom">
                            <div class="col-xs-12 col-sm-3 col-md-3 col-lg-3">
                                <div class="color-name">
                                    <h4>Colors to avoid</h4>
                                </div>
                            </div>
                            <div class="col-xs-12 col-sm-9 col-md-9 col-lg-9">
                                <% for(var i=0; i < user.colors.avoids.length; i++) { %>
                                    <div class="col-xs-1 col-sm-1 col-md-1 col-lg-1">
                                        <div class="color-section">
                                            <span class="color-section" style='background-color: <%= user.colors.avoids[i] %> !important'></span>
                                        </div>
                                    </div>
                                    <% } %>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js"></script>


</body>

</html>